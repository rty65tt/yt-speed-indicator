{
   "manifest_version": 2,
   "version": "1.0",
   "default_locale": "en",
   "name": "__MSG_appName__",
   "short_name": "youtubespeedindicator",
   "description": "__MSG_appDescription__",
   "homepage_url": "https://github.com/rty65tt/yt-speed-indicator",
  
   "icons": {
      "16": "icons/icon.svg"
   },

   "content_scripts": [ {
      "matches": [ "*://*.youtube.com/*" ],
      "all_frames": true,
      "css": [ "css/inject.css" ],
      "js": [ "inject.js" ]
   } ],

   "permissions": [
      "tabs",
      "activeTab",
      "*://*.youtube.com/*"
   ]
}
